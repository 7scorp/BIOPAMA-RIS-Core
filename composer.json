{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.4",
        "drupal/addtoany": "^1.10",
        "drupal/admin_content_notification": "^3.4",
        "drupal/admin_toolbar": "^1.24",
        "drupal/advagg": "^4.0",
        "drupal/allowed_formats": "^1.1",
        "drupal/auth0": "^2.1",
        "drupal/auto_entitylabel": "2.x-dev",
        "drupal/avatars": "^1.0",
        "drupal/backup_migrate": "^4.0",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/bootstrap_barrio": "^4.17",
        "drupal/bootstrap_library": "^1.11",
        "drupal/cas": "^1.4",
        "drupal/chosen": "^2.6",
        "drupal/console": "~1.0",
        "drupal/core": "^8.6",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^2.0",
        "drupal/domain": "^1.0",
        "drupal/domain_theme_switch": "^1.4",
        "drupal/drush_language": "^1.0",
        "drupal/ds": "^3.1",
        "drupal/easy_install": "^10.1",
        "drupal/entity": "^1.0",
        "drupal/entity_browser": "2.1",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/entity_reference_revisions": "^1.5",
        "drupal/eu_cookie_compliance": "^1.7",
        "drupal/fac": "^1.1",
        "drupal/facets": "^1.1",
        "drupal/fancy_login": "^2.2",
        "drupal/feeds": "3.x-dev",
        "drupal/feeds_ex": "^1.0@alpha",
        "drupal/fico": "^1.0",
        "drupal/field_group": "^3.0",
        "drupal/file_browser": "^1.1",
        "drupal/filefield_paths": "1.x-dev",
        "drupal/form_mode_manager": "^2.0",
        "drupal/geofield": "^1.0@beta",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^1.29",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/libraries": "^3.0@alpha",
        "drupal/login_emailusername": "^1.1",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menu_link_modal": "^1.3",
        "drupal/module_filter": "^3.1",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.3",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.3",
        "drupal/prepopulate": "2.x-dev",
        "drupal/recaptcha": "^2.4",
        "drupal/redirect": "^1.2",
        "drupal/restui": "^1.16",
        "drupal/rules": "^3.0@alpha",
        "drupal/rules_role_email": "^1.0",
        "drupal/search_api": "^1.10",
        "drupal/search_api_autocomplete": "^1.1",
        "drupal/search_api_location": "^1.0@alpha",
        "drupal/search_api_solr": "^2.2",
        "drupal/simple_oauth": "^3",
        "drupal/simplesamlphp_auth": "3.x-dev",
        "drupal/social_login": "^2.83",
        "drupal/superfish": "^1.2",
        "drupal/svg_image": "^1.8",
        "drupal/tmgmt_deepl": "^1.1",
        "drupal/tmgmt_google": "^1.0",
        "drupal/token": "^1.5",
        "drupal/toolbar_anti_flicker": "^2.7",
        "drupal/toolbar_menu": "^2.1",
        "drupal/twig_tweak": "^2.1",
        "drupal/unlimited_number": "^1.0@beta",
        "drupal/view_mode_page": "^3.1",
        "drupal/views_accordion": "^1.1",
        "drupal/views_attach_library": "^1.0",
        "drupal/views_block_filter_block": "^1.0@beta",
        "drupal/views_bulk_edit": "^2.2",
        "drupal/views_contextual_filters_or": "^1.0",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/views_xml_backend": "^1.0@alpha",
        "drupal/we_megamenu": "^1.3",
        "drupal/webform": "^5.3",
        "drupal/workbench_email": "^1.1",
        "drupal/workbench_moderation_actions": "^1.0@alpha",
        "drupal/workflow": "^1.1",
        "drupal/yearonly": "^1.3",
        "drush/drush": "^9.0.0",
        "peekmo/jsonpath": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0",
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
		"enable-patching": true,
		"patches": {
			"drupal/core": {
				"remove migrate tables": "https://www.drupal.org/files/issues/2019-04-12/2713327-100.patch",
				"ViewsEntitySchemaSubscriber should not make an entity update fail if a view cannot be resaved": "https://www.drupal.org/files/issues/2019-05-06/3052492-13.patch"
			}
		},
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
